{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5030",
    "UpstreamHeaderTransform": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
      "Access-Control-Allow-Headers": "Content-Type",
      "Access-Control-Allow-Credentials": "true"
    }
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5173
        }
      ],
      "UpstreamPathTemplate": "/",
      "UpstreamHttpMethod": [
        "GET"
      ]
    },
    {
      "DownstreamPathTemplate": "/editor/create-chart",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 3001
        }
      ],
      "UpstreamPathTemplate": "/create-chart",
      "UpstreamHttpMethod": [
        "POST"
      ]
    },
    {
      "DownstreamPathTemplate": "/dashboard/all-charts",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 3001
        }
      ],
      "UpstreamPathTemplate": "/all-charts",
      "UpstreamHttpMethod": [
        "GET"
      ]
    },
    {
      "DownstreamPathTemplate": "/editor/chart",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 3001
        }
      ],
      "UpstreamPathTemplate": "/chart",
      "UpstreamHttpMethod": [
        "GET"
      ]
    },
    {
      "DownstreamPathTemplate": "/delete-chart/:id",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 3001
        }
      ],
      "UpstreamPathTemplate": "/delete-chart/:id",
      "UpstreamHttpMethod": [
        "DELETE"
      ]
    },
    {
      "DownstreamPathTemplate": "/update-chart/:id",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 3001
        }
      ],
      "UpstreamPathTemplate": "/update-chart/:id",
      "UpstreamHttpMethod": [
        "PUT"
      ]
    },
    {
      "DownstreamPathTemplate": "/export",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 3002
        }
      ],
      "UpstreamPathTemplate": "/export",
      "UpstreamHttpMethod": [
        "GET"
      ]
    },
    {
      "DownstreamPathTemplate": "/import",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 3002
        }
      ],
      "UpstreamPathTemplate": "/import",
      "UpstreamHttpMethod": [
        "POST"
      ]
    },
    {
      "DownstreamPathTemplate": "/swagger/{documentName}/swagger.json",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 3001
        }
      ],
      "UpstreamPathTemplate": "/swagger/docs/{documentName}/swagger.json",
      "UpstreamHttpMethod": [
        "GET"
      ]
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "service1",
      "Config": [
        {
          "Name": "Service 1 API",
          "Version": "v1",
          "Url": "http://localhost:3001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "service2",
      "Config": [
        {
          "Name": "Service 2 API",
          "Version": "v1",
          "Url": "http://localhost:3002/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "SwaggerForOcelot": {
    "Routes": [
      {
        "DownstreamSwaggerEndPointBasePath": "/swagger/docs",
        "Service": "service1",
        "Path": "/swagger/docs/service1/swagger.json"
      },
      {
        "DownstreamSwaggerEndPointBasePath": "/swagger/docs",
        "Service": "service2",
        "Path": "/swagger/docs/service2/swagger.json"
      }
    ]
  }
}
