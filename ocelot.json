  "SwaggerEndPoints": [
    {
      "Key": "service1",
      "Config": [
        {
          "Name": "Service 1 API",
          "Version": "v1",
          "Url": "http://localhost:3001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "service2",
      "Config": [
        {
          "Name": "Service 2 API",
          "Version": "v1",
          "Url": "http://localhost:3002/swagger/v1/swagger.json"
        }
